                    }
                ],
                "body": {
                    "mode": "raw",
                    "raw": "{\n  \"firstName\": \"Janusz\",\n  \"lastName\": \"Kowalski\",\n  \"birthdate\": \"1989-12-10\",\n  \"email\": \"janusz.kowalski@example.com\"\n}"
