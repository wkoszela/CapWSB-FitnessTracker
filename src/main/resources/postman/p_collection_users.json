{
    "info": {
        "name": "FitnessTracker Users API",
        "_postman_id": "users-api-collection",
        "description": "Collection to test the Users API endpoints of the FitnessTracker application.",
        "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
    },
    "item": [
        {
            "name": "Get All Users",
            "request": {
                "method": "GET",
                "header": [],
                "url": {
                    "raw": "http://localhost:8091/v1/users",
                    "protocol": "http",
                    "host": [
                        "localhost"
                    ],
                    "port": "8091",
                    "path": [
                        "v1",
                        "users"
                    ]
                }
            }
        },
        {
            "name": "Get All Simple Users",
            "request": {
                "method": "GET",
                "header": [],
                "url": {
                    "raw": "http://localhost:8091/v1/users/simple",
                    "protocol": "http",
                    "host": [
                        "localhost"
                    ],
                    "port": "8091",
                    "path": [
                        "v1",
                        "users",
                        "simple"
                    ]
                }
            }
        },
        {
            "name": "Get User By ID",
            "request": {
                "method": "GET",
                "header": [],
                "url": {
                    "raw": "http://localhost:8091/v1/users/{{userId}}",
                    "protocol": "http",
                    "host": [
                        "localhost"
                    ],
                    "port": "8091",
                    "path": [
                        "v1",
                        "users",
                        "{{userId}}"
                    ]
                }
            }
        },
        {
            "name": "Get User By Email",
            "request": {
                "method": "GET",
                "header": [],
                "url": {
                    "raw": "http://localhost:8091/v1/users/email?email={{email}}",
                    "protocol": "http",
                    "host": [
                        "localhost"
                    ],
                    "port": "8091",
                    "path": [
                        "v1",
                        "users",
                        "email"
                    ],
                    "query": [
                        {
                            "key": "email",
                            "value": "{{email}}"
                        }
                    ]
                }
            }
        },
        {
            "name": "Search Users By Email Fragment",
            "request": {
                "method": "GET",
                "header": [],
                "url": {
                    "raw": "http://localhost:8091/v1/users/search/email?fragment={{fragment}}",
                    "protocol": "http",
                    "host": [
                        "localhost"
                    ],
                    "port": "8091",
                    "path": [
                        "v1",
                        "users",
                        "search",
                        "email"
                    ],
                    "query": [
                        {
                            "key": "fragment",
                            "value": "{{fragment}}"
                        }
                    ]
                }
            }
        },
        {
            "name": "Get Users Older Than Age",
            "request": {
                "method": "GET",
                "header": [],
                "url": {
                    "raw": "http://localhost:8091/v1/users/olderThan?age={{age}}",
                    "protocol": "http",
                    "host": [
                        "localhost"
                    ],
                    "port": "8091",
                    "path": [
                        "v1",
                        "users",
                        "olderThan"
                    ],
                    "query": [
                        {
                            "key": "age",
                            "value": "{{age}}"
                        }
                    ]
                }
            }
        },
        {
            "name": "Get Users Older Than Date",
            "request": {
                "method": "GET",
                "header": [],
                "url": {
                    "raw": "http://localhost:8091/v1/users/older/{{date}}",
                    "protocol": "http",
                    "host": [
                        "localhost"
                    ],
                    "port": "8091",
                    "path": [
                        "v1",
                        "users",
                        "older",
                        "{{date}}"
                    ]
                }
            }
        },
        {
            "name": "Add User",
            "request": {
                "method": "POST",
                "header": [
                    {
                        "key": "Content-Type",
                        "value": "application/json"
                    }
                ],
                "body": {
                    "mode": "raw",
                    "raw": "{\n  \"firstName\": \"Jan\",\n  \"lastName\": \"Kowalski\",\n  \"birthdate\": \"1990-05-15\",\n  \"email\": \"jan.kowalski@example.com\"\n}"
                },
                "url": {
                    "raw": "http://localhost:8091/v1/users",
                    "protocol": "http",
                    "host": [
                        "localhost"
                    ],
                    "port": "8091",
                    "path": [
                        "v1",
                        "users"
                    ]
                }
            }
        },
        {
            "name": "Update User",
            "request": {
                "method": "PUT",
                "header": [
                    {
